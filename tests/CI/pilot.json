{
  "Setups": {
    "DIRAC-Certification": {
      "CheckVersion": "True",
      "Commands": {
        "TEST": [
          "CheckWorkerNode",
          "InstallDIRAC",
          "ConfigureBasics",
          "CheckCECapabilities",
          "CheckWNCapabilities",
          "ConfigureSite",
          "ConfigureArchitecture",
          "ConfigureCPURequirements"
        ],
        "TEST-MP": [
          "CheckWorkerNode",
          "InstallDIRAC",
          "ConfigureBasics",
          "CheckCECapabilities",
          "CheckWNCapabilities",
          "ConfigureSite",
          "ConfigureArchitecture",
          "ConfigureCPURequirements"
        ],
        "TEST-FULL": [
          "CheckWorkerNode",
          "InstallDIRAC",
          "ConfigureBasics",
          "CheckCECapabilities",
          "CheckWNCapabilities",
          "ConfigureSite",
          "ConfigureArchitecture",
          "ConfigureCPURequirements",
          "LaunchAgent"
        ]
      },
      "Logging": {
        "LoggingType":"REST_API", 
        "LocalOutputFile":"myFile",
        "HostKey":"/home/dirac/certs/hostkey.pem", 
        "HostCertificate":"/home/dirac/certs/hostcert.pem",
        "CACertificate":"/etc/pki/tls/certs/ca-bundle.crt", 
        "Url":"https://pilotloggerrest.cern.ch:8888"
      },
      "NagiosProbes": "WN-mjf, WN-sft-csh, WN-sft-voms, WN-sft-vo-swdir",
      "Version": "VAR_DIRAC_VERSION",
      "NagiosPutURL": "https://lbvobox06.cern.ch:9132/sam/"
    },
    "Defaults": {
      "Commands": {
        "defaultList": [
          "CheckWorkerNode",
          "InstallDIRAC",
          "ConfigureBasics",
          "CheckCECapabilities",
          "CheckWNCapabilities",
          "ConfigureSite",
          "ConfigureArchitecture",
          "ConfigureCPURequirements"
        ],
        "Defaults": [
          "CheckWorkerNode",
          "InstallDIRAC",
          "ConfigureBasics",
          "CheckCECapabilities",
          "CheckWNCapabilities",
          "ConfigureSite",
          "ConfigureArchitecture",
          "ConfigureCPURequirements",
          "LaunchAgent"
        ]
      },
      "ConfigurationServer": "VAR_CS",
      "GenericPilotGroup": "dirac_pilot",
      "GenericPilotDN": "VAR_USERDN"
    }
  },
  "CEs": {
    "jenkins.cern.ch": {
      "Site": "VAR_JENKINS_SITE",
      "Queue": "VAR_JENKINS_QUEUE",
      "GridCEType": "TEST"
    },
    "jenkins-mp.cern.ch": {
      "Site": "VAR_JENKINS_SITE",
      "Queue": "VAR_JENKINS_QUEUE",
      "GridCEType": "TEST-MP"
    },
    "jenkins-mp-full.cern.ch": {
      "Site": "VAR_JENKINS_SITE",
      "Queue": "VAR_JENKINS_QUEUE",
      "GridCEType": "TEST-FULL"
    },
    "jenkins-mp-pool.cern.ch": {
      "Site": "VAR_JENKINS_SITE",
      "Queue": "VAR_JENKINS_QUEUE",
      "GridCEType": "TEST-FULL",
      "LocalCEType": "Pool"
    },
    "jenkins-mp-pool-sudo.cern.ch": {
      "Site": "VAR_JENKINS_SITE",
      "Queue": "VAR_JENKINS_QUEUE",
      "GridCEType": "TEST-FULL",
      "LocalCEType": "Pool",
      "SudoExecution": "True"
    }
  },
  "DefaultSetup": "DIRAC-Certification"
}
